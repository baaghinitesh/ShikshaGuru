# ShikshaGuru - Tutoring Marketplace Platform Environment Configuration
# This file configures the development environment for the Next.js tutoring application

# Command to install or update dependencies, will execute each time a new thread created to ensure dependencies up-to-date
dependency_command: npm install && cd backend && npm install

# Commands to run when "Run" button clicked
run_command:
  - npm run dev
  - cd backend && npm run dev
